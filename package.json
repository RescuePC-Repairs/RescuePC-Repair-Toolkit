{
  "name": "rescuepc-repairs-ai",
  "version": "2.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.3",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.15.3"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest --testPathPattern=security",
    "test:fix": "jest --updateSnapshot",
    "type-check": "tsc --noEmit",
    "security:audit": "npm audit && node security-audit.js",
    "performance:analyze": "ANALYZE=true pnpm build",
    "performance:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "performance:budget": "webpack-bundle-analyzer .next/static/chunks/*.js",
    "performance:compress": "gzip-size .next/static/chunks/*.js",
    "performance:monitor": "node scripts/performance-monitor.js",
    "performance:optimize": "pnpm run performance:analyze && pnpm run performance:lighthouse",
    "build:optimized": "NODE_ENV=production pnpm build",
    "start:optimized": "NODE_ENV=production pnpm start",
    "test:email": "node test-webhook.js"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@stripe/stripe-js": "^2.4.0",
    "@types/nodemailer": "^6.4.17",
    "@types/stripe": "^8.0.417",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.1",
    "abort-controller": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "critters": "^0.0.23",
    "cross-fetch": "^4.1.0",
    "csrf": "^3.1.0",
    "dotenv": "^17.0.1",
    "formdata-node": "^6.0.3",
    "html-escaper": "^3.0.3",
    "isomorphic-dompurify": "^2.25.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "next": "14.1.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.18",
    "react-error-boundary": "^4.1.2",
    "stripe": "^14.10.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.30.1",
    "@next/bundle-analyzer": "^14.1.0",
    "@playwright/test": "^1.53.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/html-escaper": "^3.0.4",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.10.4",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.16",
    "compression-webpack-plugin": "^11.1.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^2.1.0",
    "gzip-size": "^7.0.0",
    "husky": "^8.0.3",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.6.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.32",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.2.4",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.3.6",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ],
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}

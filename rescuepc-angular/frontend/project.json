{
  "name": "frontend",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "frontend/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/frontend/browser",
        "index": "frontend/src/index.html",
        "main": "frontend/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "frontend/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "frontend/public"
          },
          {
            "glob": "favicon.ico",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": "favicon-16x16.png",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": "favicon-32x32.png",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": "favicon.svg",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": "apple-touch-icon.png",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": "RescuePC-Repairs-Logo.png",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": "RescuePC-Repairs-Logo-backup.png",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": "site.webmanifest",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": "manifest.json",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": "safari-pinned-tab.svg",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": "icon.svg",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": "browserconfig.xml",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": "BingSiteAuth.xml",
            "input": "frontend/public",
            "output": "."
          },
          {
            "glob": ".well-known/**/*",
            "input": "frontend/public/.well-known",
            "output": ".well-known"
          },
          {
            "glob": "assets/**/*",
            "input": "frontend/public/assets",
            "output": "assets"
          }
        ],
        "styles": ["frontend/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "frontend:build:production"
        },
        "development": {
          "buildTarget": "frontend:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "frontend:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "frontend/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "frontend:build",
        "port": 4200,
        "spa": true
      }
    },
    "server": {
      "dependsOn": ["build"],
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/frontend/server",
        "main": "frontend/server.ts",
        "tsConfig": "frontend/tsconfig.server.json"
      },
      "configurations": {
        "production": {
          "outputHashing": "media"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false,
          "vendorChunk": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "executor": "@angular-devkit/build-angular:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "frontend:build:development",
          "serverTarget": "frontend:server:development"
        },
        "production": {
          "browserTarget": "frontend:build:production",
          "serverTarget": "frontend:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "prerender": {
      "executor": "@angular-devkit/build-angular:prerender",
      "options": {
        "routes": ["/"]
      },
      "configurations": {
        "development": {
          "browserTarget": "frontend:build:development",
          "serverTarget": "frontend:server:development"
        },
        "production": {
          "browserTarget": "frontend:build:production",
          "serverTarget": "frontend:server:production"
        }
      },
      "defaultConfiguration": "production"
    }
  }
}
